<% flash.each do |key, value| %>

  <% if key == "alert" %>
  	<script type="text/javascript">
  		swal("Uh-oh..","<%= value %>", "error");
  	</script>
  <% else %>
    <script type="text/javascript">
      swal({
        title: "<%= value %>",
        type: "success"
      });
    </script>
  <% end %>
